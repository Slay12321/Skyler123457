local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Charmed", "Ocean")

-- Unlocker
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")
local S = Window:NewTab("Others")
Local s = Main:NewSection("Misc")

MainSection:NewButton("Temporal Status", "Z", function()
     game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
 if key == "z" then
    local args = {
        [1] = "Temporal"
    }
    
    game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))
    
end
end)
end)


MainSection:NewButton("Piper Combust", "F", function()
    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
        if key == "f" then
                             local args = {
           [1] = "Molecular Combustion"
       }
       
       game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))
       
        end
       end)
end)

MainSection:NewButton("Energy Wave", "C", function()
    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(key)
        if key == "C" then
            local args = {
                [1] = "Energy Waves"
            }
            
            game:GetService("ReplicatedStorage").AbilityRemote:FireServer(unpack(args))
        end
       end)
end)


